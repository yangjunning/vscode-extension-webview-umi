{
	"name": "webview-umi",
	"displayName": "webview-umi",
	"description": "webview umi vscode extension demo",
	"version": "0.0.1",
	"private": "true",
	"workspaces": [
		"web"
	],
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:webview-umi.projectManager.start"
	],
	"main": "./out/extension.js",
	"publisher": "youngjuning",
	"contributes": {
		"commands": [
			{
				"command": "webview-umi.projectManager.start",
				"title": "webview-umi:项目管理"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn build:web && yarn compile",
		"build:web": "yarn workspace web run build",
    "watch:web": "yarn workspace web run start",
		"precompile": "rimraf out",
		"prewatch": "rimraf out",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn compile && yarn lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"url": "https://github.com/youngjuning/vscode-extension-webview-umi"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
